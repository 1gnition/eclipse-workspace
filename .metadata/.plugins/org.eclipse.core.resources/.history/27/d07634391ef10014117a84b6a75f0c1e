package an;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

@Configuration
public class AppConfig {
	@Bean(name="bar")
	public Bar getBar() {
		Bar bar = new Bar();
		bar.setString("123");
		System.out.println(bar);
		return bar;
	}
	
	@Bean(name="foo")
	public Foo getFoo() {
		Foo foo = new Foo();
		foo.setBar(getBar());
		return foo;
	}
}