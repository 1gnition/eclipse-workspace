<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<appFrameworkConfig xmlns:ns2="http://www.ff4j.org/schema/ff4j">
  <accessManagement>
    <permissions>
      <permission id="1"
        name="o_dashboard_application_name:o_dashboard_application_name:access" />
      <permission id="2"
        name="o_dashboard_application_name:o_dashboard_application_name:edit" />
      <permission id="3"
        name="o_uua_application_name:o_uua_application_name:access" />
      <permission id="4" name="liferay:liferay:administration" />
      <permission id="5" name="useradmin:user:view" />
      <permission id="6" name="useradmin:user:edit" />
    </permissions>
    <roles>
      <role id="professional services" name="professional services">
        <permissionsRef>
          <permissionRef id="1" />
          <permissionRef id="2" />
          <permissionRef id="3" />
          <permissionRef id="4" />
          <permissionRef id="5" />
          <permissionRef id="6" />
        </permissionsRef>
      </role>
      <role id="support" name="support">
        <permissionsRef>
          <permissionRef id="1" />
          <permissionRef id="2" />
          <permissionRef id="3" />
          <permissionRef id="5" />
          <permissionRef id="6" />
        </permissionsRef>
      </role>
      <role id="administrator" name="administrator">
        <permissionsRef>
          <permissionRef id="1" />
          <permissionRef id="2" />
          <permissionRef id="3" />
          <permissionRef id="5" />
          <permissionRef id="6" />
        </permissionsRef>
      </role>
    </roles>
  </accessManagement>
  <userManagement>
    <userAttributes>
      <userAttribute name="FIRST_NAME" type="STRING">
        <ui title="First Name" section="uua-new-user-section-1"
          tooltip="User First Name" readOnly="true" sortOrder="10"
          isVisiable="true" />
        <rules>
          <rule isError="true" validationType="REQUIRED"
            validationCalcValue="" />
          <rule isError="true" validationType="MAX_SIZE"
            validationCalcValue="75" />
        </rules>
      </userAttribute>
      <userAttribute name="LAST_NAME" type="STRING">
        <ui title="Last Name" section="uua-new-user-section-1"
          tooltip="User Last Name" readOnly="true" sortOrder="30"
          isVisiable="true" />
        <rules>
          <rule isError="true" validationType="REQUIRED"
            validationCalcValue="" />
          <rule isError="true" validationType="MAX_SIZE"
            validationCalcValue="75" />
        </rules>
      </userAttribute>
      <userAttribute name="LOGIN_NAME" type="STRING">
        <ui title="Application Login" section="uua-new-user-section-1"
          tooltip="Application Login" readOnly="true" sortOrder="60"
          isVisiable="true" />
        <rules>
          <rule isError="false" validationType="IS_UNIQUE"
            validationCalcValue="" />
        </rules>
      </userAttribute>
      <userAttribute name="WORKSTATION_LOGIN" type="STRING">
        <ui title="Workstation Login" section="uua-new-user-section-1"
          tooltip="Workstation Login" readOnly="true" sortOrder="70"
          isVisiable="true" />
        <rules>
          <rule isError="false" validationType="IS_UNIQUE"
            validationCalcValue="" />
        </rules>
      </userAttribute>
      <userAttribute name="EMAIL" type="STRING">
        <ui title="Email" section="uua-new-user-section-2" tooltip="Email"
          readOnly="true" sortOrder="50" isVisiable="true" />
        <rules>
          <rule isError="true" validationType="VALID_EMAIL"
            validationCalcValue="" />
          <rule isError="false" validationType="IS_UNIQUE"
            validationCalcValue="" />
          <rule isError="true" validationType="MAX_SIZE"
            validationCalcValue="75" />
        </rules>
      </userAttribute>
      <userAttribute name="HIRE_DATE" type="DATETIME">
        <ui title="Hire Date" section="uua-new-user-section-2"
          tooltip="Hire Date" readOnly="true" sortOrder="80" isVisiable="true" />
      </userAttribute>
      <userAttribute name="SUFFIX" type="STRING">
        <ui title="Suffix" section="uua-new-user-section-2" tooltip="Suffix"
          readOnly="true" sortOrder="40" isVisiable="true" />
      </userAttribute>
      <userAttribute name="MIDDLE" type="STRING">
        <ui title="Middle Name" section="uua-new-user-section-1"
          tooltip="Middle Name" readOnly="true" sortOrder="20"
          isVisiable="true" />
        <rules>
          <rule isError="true" validationType="MAX_SIZE"
            validationCalcValue="75" />
        </rules>
      </userAttribute>
    </userAttributes>
  </userManagement>
  <configuration>
    <configurationItem
      key="|OAPP|Authorization|PermissionsChangedTopicName|" type="STRING">
      <value>PermissionChangeNotification</value>
      <caption></caption>
      <description></description>
    </configurationItem>
    <configurationItem key="|OAPP|Authorization|PermissionsProviderURI|"
      type="STRING">
      <value>/ccih-uua-web/uua-services/permissions/getRolePermissionsByBulks</value>
      <caption></caption>
      <description></description>
    </configurationItem>
    <configurationItem
      key="|UUA|UserSettings|AmountOfDaysToDetectIdleUsers|" type="INT">
      <value>0</value>
      <caption>Deactivate account after [X] days of inactivity</caption>
      <description>Amount of days of inactivity after which the user
        account will be deactivated</description>
    </configurationItem>
  </configuration>
  <ns2:features>
    <ns2:feature uid="FEATURE_ON" enable="true" />
    <ns2:feature uid="FEATURE_OFF" enable="false" />
  </ns2:features>
</appFrameworkConfig>